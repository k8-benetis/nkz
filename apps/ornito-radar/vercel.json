{
  "framework": "vite",
  "buildCommand": "cd ../.. && pnpm install --no-frozen-lockfile --shamefully-hoist && pnpm --filter @nekazari/sdk run build && pnpm --filter @nekazari/ui-kit run build && cd apps/ornito-radar && vite build",
  "outputDirectory": "dist",
  "installCommand": "cd ../.. && pnpm install --no-frozen-lockfile --shamefully-hoist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
